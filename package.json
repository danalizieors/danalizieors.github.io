{
    "name": "danalizieors.github.io",
    "version": "0.1.0",
    "description": "Resume site",
    "repository": "git@github.com:danalizieors/danalizieors.github.io.git",
    "author": "Danalizi-Erőss Örs <danalizieors@gmail.com>",
    "private": true,
    "scripts": {
        "format": "prettier --write .",
        "verify:compile": "tsc",
        "verify:format": "prettier --check .",
        "verify:test": "yarn test",
        "verify": "run-s verify:*",
        "test": "jest --config .jestrc.json --passWithNoTests",
        "start": "parcel source/index.html --cache-dir build/cache --out-dir build/start",
        "build:local": "parcel build source/index.html --cache-dir build/cache --out-dir build/local",
        "build:deploy": "parcel build source/index.html --cache-dir build/cache --out-dir build/deploy --public-url /",
        "build": "run-p build:*",
        "clean": "rm -rf build",
        "serve": "serve --listen 1234 build/local"
    },
    "dependencies": {
        "@chakra-ui/react": "^1.6.1",
        "@emotion/react": "^11.4.0",
        "@emotion/styled": "^11.3.0",
        "focus-visible": "^5.2.0",
        "framer-motion": "^4.1.13",
        "ramda": "^0.27.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-icons": "^4.2.0",
        "react-router-dom": "^5.2.0",
        "zustand": "^3.5.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@types/ramda": "^0.27.40",
        "@types/react": "^17.0.5",
        "@types/react-dom": "^17.0.3",
        "@types/react-router-dom": "^5.1.7",
        "husky": "^4.3.0",
        "jest": "^26.6.3",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.5",
        "prettier": "^2.3.0",
        "prettier-plugin-organize-imports": "^2.0.0",
        "serve": "^11.3.2",
        "ts-jest": "^26.5.6",
        "typescript": "^4.2.4"
    }
}
